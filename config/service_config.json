{
  "service_config": {
    "service": {
      "name": "ppocrv5_onnx_service",
      "version": "1.0.0",
      "port": 8000,
      "max_batch_size": 8,
      "timeout_ms": 30000,
      "log_level": "INFO",
      "thread_pool_size": 4
    },
    "model": {
      "det_model": {
        "path": "./models/ch_PP-OCRv5_det_infer.onnx",
        "input_names": ["x"],
        "output_names": ["sigmoid_0.tmp_0"],
        "input_shape": [1, 3, -1, -1],
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225],
        "is_bgr": true,
        "min_size": 32,
        "max_size": 1536
      },
      "rec_model": {
        "path": "./models/ch_PP-OCRv5_rec_infer.onnx",
        "input_names": ["x"],
        "output_names": ["softmax_5.tmp_0"],
        "input_shape": [1, 3, 48, -1],
        "mean": [0.5, 0.5, 0.5],
        "std": [0.5, 0.5, 0.5],
        "is_bgr": true,
        "rec_image_height": 48,
        "rec_batch_num": 6
      },
      "cls_model": {
        "path": "./models/ch_ppocr_mobile_v2.0_cls_infer.onnx",
        "input_names": ["x"],
        "output_names": ["softmax_0.tmp_0"],
        "input_shape": [1, 3, 48, 192],
        "mean": [0.5, 0.5, 0.5],
        "std": [0.5, 0.5, 0.5],
        "is_bgr": true,
        "cls_thresh": 0.9
      },
      "character_dict": {
        "path": "./models/ppocr_keys_v1.txt",
        "dict_size": 6625
      },
      "postprocess": {
        "det_db_thresh": 0.3,
        "det_db_box_thresh": 0.6,
        "det_db_unclip_ratio": 1.5,
        "max_text_length": 25,
        "rec_score_thresh": 0.5
      }
    }
  }
}